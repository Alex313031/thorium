PATH_COLOR_ARGB, 0xFF, 0xE5, 0x1C, 0x23,
STROKE, 1.85f,
MOVE_TO, 4, 4,
R_LINE_TO, 8, 8,
MOVE_TO, 4, 12,
R_LINE_TO, 8, -8
